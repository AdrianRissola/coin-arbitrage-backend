
{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": 0,
    "arrow-parens": 0,
    "no-tabs": 0,
    "prettier/prettier": ["error",   {
      "endOfLine": "auto"
    }]
  }
}